class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
        unordered_map<int,int>mp;
        for(int i:nums)
        {
            mp[i]++;
        }

        vector<pair<int,int>> vec;

for(auto p : mp){
    vec.push_back({p.second, p.first});
}

sort(vec.rbegin(), vec.rend());
  // Take top k elements
        vector<int> ans;

         // take top k elements
        for(int i = 0; i < k; i++){
            ans.push_back(vec[i].second);
        }
        return ans;


    }
};




class Solution {
public:
    typedef pair<int,int>p;
    vector<int> topKFrequent(vector<int>& nums, int k) {
        unordered_map<int,int>mp;
        for(int i:nums)
        {
            mp[i]++;
        }

        priority_queue<p,vector<p>,greater<p>>pq;
        
        for(auto &it:mp)
        {
            int val=it.first;
            int freq=it.second;
             
            pq.push({freq,val});

            if(pq.size()>k)
            {
                pq.pop();
            }
        }


        vector<int>ans;

       while(!pq.empty())
       {
        ans.push_back(pq.top().second);
        pq.pop();
       }
        return ans;


    }
};
