n array tree is a tree that can have more than 2 children.


/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val) {
        val = _val;
    }

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
public:

    void func( vector<int>&ans,Node* root)
    {
        for(int i=0;i<root->children.size();i++)  // visit first all children abd print
        {
            func(ans,root->children[i]);
        }
        ans.push_back(root->val);            // then print the root
    }
    vector<int> postorder(Node* root) {
        vector<int>ans;
        if(!root)
        return ans;
        func(ans,root);
        return ans;

    }
};
