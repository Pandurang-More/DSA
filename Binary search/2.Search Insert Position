Example 1:

Input: nums = [1,3,5,6], target = 5  
Output: 2  

Example 2:

Input: nums = [1,3,5,6], target = 2  
Output: 1  

Example 3:

Input: nums = [1,3,5,6], target = 7  
Output: 4  

--------------------------------------------------

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {

        int start = 0;
        int end = nums.size() - 1;

        while (start <= end) {

            int mid = start + (end - start) / 2;

            if (nums[mid] == target)
                return mid;
            else if (target < nums[mid])
                end = mid - 1;
            else
                start = mid + 1;
        }

        return start;
    }
};
