#include<iostream>
using namespace std;
#include<vector>
#include<algorithm>

int partition(int arr[],int start,int end)
{
  int pos=start;
  for(int i=start;i<=end;i++)
  {
    if(arr[i]<=arr[end])
    {
      swap(arr[i],arr[pos]);   // swap the arr[pos], arr[i] if pivot is less than equal to arr[i]
      pos++;
    }
  }
  return pos-1;   // return the position of pivot element.
}

void quicksort(int arr[],int start,int end)
{
  if(start>=end)
  return;
  int pivot=partition(arr,start,end);
  quicksort( arr, start,pivot-1);  // left side element of pivot
  quicksort(arr,pivot+1,end);      // right side element of pivot
}


int main()
{    

     int arr[]={6,3,1,2,8,9,10,7,3,10};
     quicksort(arr,0,9);
     for(int i=0;i<10;i++)
     cout<<arr[i]<<" ";
     
}
